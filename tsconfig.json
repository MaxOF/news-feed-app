    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "jsx": "react",
    "target": "es5",
    "sourceMap": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
